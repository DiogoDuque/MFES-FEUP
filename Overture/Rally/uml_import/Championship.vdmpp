class Championship

types 
	string = Utils`string;

instance variables
  public series : set of (Rally) := {};
  public teams : set of (Team) := {};
  public partners : set of (Partner):= {};
  public name : string;
  public season:  int;
  
operations
  Championship : string * int ==> Championship
	Championship(n, seas) == (
	  name := n;
	  season := seas;
	  return self;
	);  

end Championship
