class Stage

types
	StageType = Utils`StageType;

instance variables
  public performances : set of (Performance) := {};
  public type : StageType;
  public date : ?;
  public distance : real;
  
operations
  Stage : StageType * real ==> Stage
	Stage(stageType, dist) == (
		type := stageType;
		distance := dist;
	  return self;
	);      
  

end Stage
