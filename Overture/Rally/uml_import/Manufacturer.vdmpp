class Manufacturer

types 
	string = Utils`string;
	Country = Utils`Country;

instance variables
  public name : string;
  public description : string;
  public country : Country;
  
operations
  Manufacturer : string * string * Country ==> Manufacturer
	Manufacturer(nm, desc, ctr) == (
	  name := nm;
	  description := desc;
	  country := ctr;
	  return self;
	);
	  
end Manufacturer
